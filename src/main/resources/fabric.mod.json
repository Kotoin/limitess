	"authors": [
		"XuP"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/limitless-enchantment/textures/limitless.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.minecraft.module.LimitlessEnchantment"
		]
	},
	"mixins": [
		"limitless-enchantment.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.4",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
